<%= simple_form_for @product do |f| %>
<%= f.label :"Subject" %>
<%= f.text_field :subject %>
<%= f.label :"Name" %>
<%= f.text_field :name %>
<%= f.label :"Country" %>
<%= f.text_field :country %>
<%= f.label :"Description" %>
<%= f.text_field :description %>
<%= f.label :"Place" %>
<%= f.text_field :place %>

<%= f.submit "Submit", class: "btn btn-primary" %>
<%end%>
